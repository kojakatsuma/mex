{
  "name": "mex",
  "version": "1.0.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects. Built with typescript.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 8080",
    "lint-app": "tslint --project",
    "typecheck-app": "tsc --project",
    "builds": "gcloud builds submit --tag gcr.io/caramel-spot-275216/mex",
    "deploy": "gcloud run deploy mex --image=gcr.io/caramel-spot-275216/mex --platform=managed --region=us-central1",
    "makelog": "node scripts/makelog.js",
    "maketext": "node scripts/maketext.js "
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.5",
    "luxon": "^1.24.1",
    "next": "^9.4.4",
    "prism-themes": "^1.4.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-notion": "^0.6.1",
    "react-twitter-embed": "^3.0.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.10.2",
    "@mapbox/rehype-prism": "^0.4.0",
    "@next/mdx": "^9.4.4",
    "@types/luxon": "^1.24.0",
    "@types/node": "14.0.11",
    "@types/react": "^16.9.39",
    "@types/react-dom": "16.9.8",
    "remark-autolink-headings": "^6.0.0",
    "remark-slug": "^6.0.0",
    "tslint": "^5.11.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.9.5"
  }
}
