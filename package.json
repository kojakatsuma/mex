{
  "name": "mex",
  "version": "1.0.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects. Built with typescript.",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "dev": "next",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 8080",
    "lint-app": "tslint --project",
    "typecheck-app": "tsc --project",
    "maketext": "node scripts/maketext.js",
    "postmaketext": "node scripts/makesitemap.js"
  },
  "dependencies": {
    "@types/p5": "^0.9.1",
    "luxon": "^1.26.0",
    "next": "^10.0.6",
    "node-fetch": "^2.6.1",
    "p5": "^1.2.0",
    "prism-themes": "^1.5.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-notion-dev": "^0.8.4"
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.13",
    "@types/luxon": "^1.25.3",
    "@types/node": "14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-dom": "17.0.1",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.1.5"
  }
}
