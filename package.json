{
  "name": "mex",
  "version": "1.0.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects. Built with typescript.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 8080",
    "lint-app": "tslint --project",
    "typecheck-app": "tsc --project",
    "deploy": "gcloud builds submit --tag gcr.io/cloudrun-turorial/helloworld"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.5.8",
    "next": "^9.3.4",
    "next-page-transitions": "^1.0.0-beta.2",
    "prism-themes": "^1.4.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-twitter-embed": "^3.0.3"
  },
  "devDependencies": {
    "@next/mdx": "^9.3.4",
    "@mapbox/rehype-prism": "^0.4.0",
    "@types/node": "13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "16.9.6",
    "cpx": "1.5.0",
    "firebase-tools": "^8.0.2",
    "remark-autolink-headings": "^6.0.0",
    "remark-slug": "^6.0.0",
    "tslint": "^5.11.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  }
}
