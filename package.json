{
  "name": "mex",
  "version": "1.0.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects. Built with typescript.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 8080",
    "lint-app": "tslint --project",
    "typecheck-app": "tsc --project",
    "maketext": "node scripts/maketext.js",
    "postmaketext": "node scripts/makesitemap.js"
  },
  "dependencies": {
    "@types/p5": "^0.9.1",
    "luxon": "^1.25.0",
    "next": "^10.0.1",
    "node-fetch": "^2.6.1",
    "p5": "^1.1.9",
    "prism-themes": "^1.5.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-notion": "^0.8.2",
    "react-twitter-embed": "^3.0.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/luxon": "^1.25.0",
    "@types/node": "14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "16.9.9",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.0.5"
  }
}
