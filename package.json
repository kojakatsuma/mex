{
  "name": "mex",
  "version": "1.0.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects. Built with typescript.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 8080",
    "lint-app": "tslint --project",
    "typecheck-app": "tsc --project",
    "builds": "gcloud builds submit --tag gcr.io/caramel-spot-275216/mex",
    "deploy": "gcloud run deploy mex --image=gcr.io/caramel-spot-275216/mex --platform=managed --region=us-central1",
    "makelog": "node scripts/makelog.js",
    "maketext": "node scripts/maketext.js",
    "postmaketext": "node scripts/makesitemap.js"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.16",
    "@types/p5": "^0.9.1",
    "luxon": "^1.25.0",
    "next": "^9.5.3",
    "p5": "^1.1.9",
    "prism-themes": "^1.4.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-notion": "^0.8.1",
    "react-twitter-embed": "^3.0.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.11.2",
    "@mapbox/rehype-prism": "^0.5.0",
    "@next/mdx": "^9.5.4",
    "@types/luxon": "^1.25.0",
    "@types/node": "14.11.5",
    "@types/react": "^16.9.51",
    "@types/react-dom": "16.9.8",
    "remark-autolink-headings": "^6.0.1",
    "remark-slug": "^6.0.0",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.0.3",
    "xmlbuilder": "^15.1.1"
  }
}
