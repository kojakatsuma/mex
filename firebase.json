{
  "hosting": {
    "public": "out",
    "predeploy": [
      "npm run build",
      "npm run export"
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/log",
        "destination": "/log.html"
      },
      {
        "source": "/text",
        "destination": "/text.html"
      },
      {
        "source": "/text/1",
        "destination": "/text/1.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}